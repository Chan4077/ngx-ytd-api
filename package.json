{
  "name": "ngx-ytd-api-src",
  "description": "Source code of unofficial YouTube Data API for Angular",
  "homepage": "https://github.com/EdricChan03/ngx-ytd-api",
  "license": "MIT",
  "scripts": {
    "buildDemo": "npm run demo:build",
    "buildDocs": "npm run demo:build",
    "buildLib": "npm run lib:build",
    "buildLibNext": "npm run lib:buildNext",
    "lintLib": "npm run lib:lint",
    "testLib": "npm run lib:test",
    "serveDemo": "npm run demo:serve",
    "packageTags": "bash ./scripts/scripts.sh package --tags",
    "demo:build": "bash ./scripts/scripts.sh build-demo",
    "demo:serve": "ng build ngx-ytd-api-lib && ng serve ngx-ytd-api-demo",
    "lib:build": "bash ./scripts/scripts.sh build-lib",
    "lib:buildNext": "npm run lib:build -- --publishNext",
    "lib:buildSkipNpm": "npm run lib:build -- --skipNpm",
    "lib:lint": "ng lint ngx-ytd-api-lib",
    "lib:packageTags": "bash ./scripts/scripts.sh package --tags",
    "lib:test": "npm run lib:buildSkipNpm && ng test ngx-ytd-api-lib --watch=false && npm run lib:testSchematics && npm run lib:cleanSchematicsUnitTesting",
    "lib:testSchematics": "cd projects/ngx-ytd-api-lib && ts-node ../../node_modules/.bin/jasmine schematics/**/*_spec.ts",
    "lib:cleanSchematicsUnitTesting": "shx cd dist/ngx-ytd-api-lib/schematics && shx rm -rf *_spec.{d.ts,js,js.map}"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^8.0.0",
    "@angular/cdk": "^8.0.0",
    "@angular/common": "^8.0.0",
    "@angular/compiler": "^8.0.0",
    "@angular/core": "^8.0.0",
    "@angular/flex-layout": "^8.0.0-beta.26",
    "@angular/forms": "^8.0.0",
    "@angular/material": "^8.0.0",
    "@angular/platform-browser": "^8.0.0",
    "@angular/platform-browser-dynamic": "^8.0.0",
    "@angular/router": "^8.0.0",
    "core-js": "^3.1.3",
    "hammerjs": "^2.0.8",
    "rxjs": "^6.5.2",
    "zone.js": "^0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.800.0",
    "@angular-devkit/build-ng-packagr": "^0.800.0",
    "@angular-devkit/core": "^7.3.9",
    "@angular-devkit/schematics": "^8.0.0",
    "@angular/cli": "^8.0.0",
    "@angular/compiler-cli": "^8.0.0",
    "@angular/language-service": "^8.0.0",
    "@schematics/angular": "^8.0.0",
    "@types/jasmine": "^3.3.13",
    "@types/jasminewd2": "^2.0.6",
    "@types/node": "^12.0.3",
    "@types/parse5": "^5.0.0",
    "angular-cli-ghpages": "^0.5.3",
    "codelyzer": "~5.0.1",
    "gulp": "^4.0.2",
    "gulp-conventional-changelog": "^2.0.19",
    "inquirer": "^6.3.1",
    "jasmine": "^3.4.0",
    "jasmine-core": "^3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "^4.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "^2.0.5",
    "karma-jasmine": "^2.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "ng-packagr": "^5.2.0",
    "ngx-simple-http": "^1.0.0",
    "parse5": "^5.1.0",
    "protractor": "^5.4.2",
    "ts-node": "^8.2.0",
    "tslib": "^1.9.3",
    "tslint": "^5.16.0",
    "typescript": "^3.4.5"
  }
}
